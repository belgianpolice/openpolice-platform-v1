<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0">
	<name>Contact information</name>
	<author>Belgian Police</author>
	<creationDate>August 2011</creationDate>
	<copyright>Copyright (C) 2011 Timble. All rights reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<authorEmail></authorEmail>
	<authorUrl>www.lokalepolitie.be</authorUrl>
	<version>1.5.0</version>
	<description></description>
	<files>
		<filename module="mod_contact_info">mod_contact_info.php</filename>
	</files>
	<params>
		<param name="street" type="text" default="" label="Street" description="" />
		<param name="zip" type="text" default="" label="ZIP" description="" />
		<param name="city" type="text" default="" label="City" description="" />
		<param name="phone" type="text" default="" label="Phone" description="" />
		<param name="fax" type="text" default="" label="Fax" description="" />
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="link_text" type="text" default="" label="Link (Text)" description="" />
		<param name="link_url" type="text" default="" label="Link (URL)" description="" />
	</params>
	<params group="advanced">
		<param name="cache" type="list" default="1" label="Caching" description="Select whether to cache the content of this module">
			<option value="1">Use global</option>
			<option value="0">No caching</option>
		</param>
		<param name="cache_time" type="text" default="900" label="Cache Time" description="The time before the module is recached" />
	</params>
</install>
